{
  "multipart": [
    {
      "when": { "north": true },
      "apply": { "model": "processenhancement:block/wire_side" }
    },
    {
      "when": { "south": true },
      "apply": { "model": "processenhancement:block/wire_side", "y": 180 }
    },
    {
      "when": { "east": true },
      "apply": { "model": "processenhancement:block/wire_side", "y": 90 }
    },
    {
      "when": { "west": true },
      "apply": { "model": "processenhancement:block/wire_side", "y": 270 }
    },
    {
      "when": { "down": true },
      "apply": { "model": "processenhancement:block/wire_side", "x": 90 }
    },
    {
      "when": { "up": true },
      "apply": { "model": "processenhancement:block/wire_side", "x": 270 }
    },
    {
      "apply": { "model": "processenhancement:block/wire_core" }
    }
  ]
}